          <param style="query" name="fileId" type="xs:string" required="true">
            <doc title="Select the Data Exchange in Autodesk Docs" />
          </param>
          <param style="query" name="assetid" type="xs:string" required="true">
            <doc title="Property identifier for a property within the selected Data Exchange" />
          </param>
        </request>
