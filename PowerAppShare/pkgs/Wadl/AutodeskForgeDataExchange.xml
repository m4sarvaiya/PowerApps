      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="GetAssetInfo" actionName="GetAssetInfo" visibility="important" siena:isDeprecated="false" siena:isDynamicSchema="true">
        <doc title="Get details of a Data Exchange property&#xA;This operation gets details of a property associated with a Data Exchange." />
